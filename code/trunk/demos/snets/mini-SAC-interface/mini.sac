void A( SNet::SNet &hnd, float[*] A)
{
  res = A + tof(1);
  res2 = res + tof(1);
  res3 = res2 + tof(1);
  SNet::out( hnd, 1, sacprelude::wrap( res));
  SNet::out( hnd, 1, sacprelude::wrap( res2));
  SNet::out( hnd, 1, sacprelude::wrap( res3));
}

