

.PHONY: clean

all: $(TARGET)

%.c: %.h

%.o: %.c
	gcc -c $< -o $@ $(INCLUDES)


$(TARGET):  $(TARGET).snet $(BOXES) $(SNETLIBS)
	snetc $(SNETCFLAGS) $(INCLUDES) $(LIBRARIES) $(TARGET).snet -o $(TARGET) -lpel # -distributed
#	gcc -c *.c -pg -Wall $(INCLUDES) -DSNetMain__star=main
#	gcc -o $(TARGET) *.o -pg $(LIBRARIES) -lsnetlpel -lsnetutil -lC4SNet


clean:
	rm -f $(TARGET) $(TARGET).o $(TARGET).c $(TARGET).h $(BOXES) mon_*.log *tasks.map
