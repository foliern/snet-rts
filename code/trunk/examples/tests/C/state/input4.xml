<?xml version="1.0" encoding="ISO-8859-1" ?>

<!-- first the input values -->

<record type="data" mode="textual" interface="C4SNet">
  <field label="inval" >(int)1</field>
</record>

<record type="data" mode="textual" interface="C4SNet">
  <field label="inval" >(int)2</field>
</record>

<record type="data" mode="textual" interface="C4SNet">
  <field label="inval" >(int)3</field>
</record>

<record type="data" mode="textual" interface="C4SNet">
  <field label="inval" >(int)4</field>
</record>

<record type="data" mode="textual" interface="C4SNet">
  <field label="inval" >(int)5</field>
</record>

<record type="data" mode="textual" interface="C4SNet">
  <field label="inval" >(int)6</field>
</record>

<record type="data" mode="textual" interface="C4SNet">
  <field label="inval" >(int)7</field>
</record>

<!--
  Adding the initial state will process the input values until
  MAXVAL=10 is reached, after which no state will be output.

  Hence, records 5-7 will remain unmerged in synchrocells,
  which are destroyed by the termination record following the
  state record.
-->

<record type="data" mode="textual" interface="C4SNet">
  <field label="state" >(int)0</field>
</record>


<record type="terminate" />



