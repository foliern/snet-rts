  <interface name="SAC4SNet" default="true" />
</metadata>

net mini {

  <?xml version="1.0"?>
    <metadata xmlns="snet-home.org" >
      <box name="boxA" >
        <SACmodule>mini</SACmodule>
        <SACboxfun>A</SACboxfun>
      </box>
      <box name="B" >
        <SACmodule>mini</SACmodule>
        <SACboxfun>B</SACboxfun>
      </box>
    </metadata>

  box boxA( (A) -> (A));
  box B( -> (A));

} connect (B|[]) .. boxA;
