int SNetMain__test00(int argc, char* argv[])
{
    int ret = 0;

    // Initialize runtime
    SNetGlobalInitialize();

    // Create a new domain
    snet_domain_t *snetd = SNetDomainCreate();

    // Setup network
    if (!SNet__test00___setup(snetd)) {
        return -1;
    }

    // Run
    ret = SNet__test00___run(snetd);
    
    // Cleanup
    SNetDomainDestroy(snetd);
    SNetGlobalDestroy();

    return ret;
}

