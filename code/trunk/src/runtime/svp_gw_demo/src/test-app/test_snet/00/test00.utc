bool SNet__test00___setup(snet_domain_t *snetd)
{
    snet_gnode_t *root = SNetGraphCreate(snetd);

    if (SNet__test00___test00(root) == NULL) {
        return false;
    }

    SNetDomainSetGraph(snetd, root);

    return true;
}

